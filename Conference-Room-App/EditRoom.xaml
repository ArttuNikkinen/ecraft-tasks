<Window x:Class="Conference_Room_App.EditRoom"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Conference_Room_App"
        mc:Ignorable="d"
        Title="EditRoom" Height="450" Width="800"
        Background="#FC0">
    <Grid>
        <TextBox Name ="roomName" HorizontalAlignment="Left" Height="23" Margin="10,48,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Room name" HorizontalAlignment="Left" Margin="10,22,0,0" VerticalAlignment="Top"/>
        <Button Name="editRoomButton" Content="Apply changes" HorizontalAlignment="Left" Margin="9,94,0,0" VerticalAlignment="Top" Width="90" Click="EditRoomButton_Click"/>
        <Button Name="editRoomCancel" Content="Close" HorizontalAlignment="Left" Margin="9,129,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.359,-0.405" Click="EditRoomCancel_Click"/>
    </Grid>
</Window>
